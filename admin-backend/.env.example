# Environment Variables Example
# Copy this file to .env and fill in your actual values

# Database URLs (required for migration script)
DEV_DATABASE_URL=postgresql://username:password@host:port/database?sslmode=require
PROD_DATABASE_URL=postgresql://username:password@host:port/database?sslmode=require

# Alternative: Use DATABASE_URL for production (used by Vercel)
DATABASE_URL=postgresql://username:password@host:port/database?sslmode=require

# Session secret for authentication
SESSION_SECRET=your-secure-session-secret-here